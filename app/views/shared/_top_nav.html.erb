<div class="nav">
  <div class="search_bar"><%= render "shared/search" %></div>
  <%= image_tag "artspace.svg", id:"logo" %>
  <h1 id="title2">Space</h1>
  <% if current_user %>
  <%= link_to 'Home', root_url %>
  <%= link_to 'Profile', current_user %>
    <% if current_user.spaces_administrated == [] && current_user.spaces_created == [] %>
     <% else %>
     <%= link_to 'Manage', manage_path(current_user), method: :get %>
    <% end %>
  <%= link_to 'New Space', new_space_path %>
  <%= link_to 'Logout', logout_path, method: :delete %>
  <% else %>
  <%= link_to 'Login', login_path, method: :get %>
  <%= link_to 'Sign Up', signup_path, method: :get %>
  <% end %>
</div>