
  <p><%= s3_uploader_form callback_method: "POST", max_file_size: 1.megabytes, callback_param: "upload[image_url]", id: "s3-uploader" do %>
  <%= hidden_field_tag  "Content-Type", "" %>
  <%= file_field_tag :file, accept: "image/png,image/jpeg", multiple: true %></p>
  <% end %>

  <div id="uploads_container"></div>
  <div id="progress_container"></div>

<script id="template-upload" type="text/x-tmpl">
  <div id="file-{%=o.unique_id%}" class="upload" style="margin-right: auto; margin-left: auto;">
    <div class="progress"><div class="bar" style="width: 0%"></div></div>
  </div>
</script>
