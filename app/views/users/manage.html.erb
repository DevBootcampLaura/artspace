<%= render "shared/top_nav" %>
<div id="page">
  <div id="title7">CLICK ON A TITLE TO EDIT INFORMATION AND AVAILABILITY</div>
  <div id="nav_wrapper">
    <ul id="manager_nav">
      <% @spaces.each do |space| %>
      <li>
        <%= link_to "#{space.title}", "##{space.title}" %>
      </li>
      <% end %>
    </ul>
  </div>
    

  <% @spaces.each do |space| %>
    <div class="space_holder1" id="<%= space.title %>">
      <h3><% if space.active == 0 %>Inactive<% end %></h3>
      <h2 id="link"><%= link_to "#{space.title}", edit_space_path(space.id) %></h2>

    </div>
  <% end %>

    

</div>

