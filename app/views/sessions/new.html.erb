
<%= render "shared/top_nav" %>
	<%= form_tag("/login", method: "post", :class => "form") do %>
	  <p><%= label_tag(:email, "Email") %>
	  <%= text_field_tag(:email) %></p>
	  <p><%= label_tag(:password, "Password") %>
	  <%= password_field_tag(:password) %></p>
	  <%= submit_tag("Log In", class:"search_it") %>
	<% end %>
<%= render "shared/footer" %>
